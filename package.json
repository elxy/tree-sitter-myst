{
  "name": "tree-sitter-myst",
  "version": "0.0.1",
  "description": "MyST grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "markdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/elxy/tree-sitter-myst.git"
  },
  "author": "elxy (https://github.com/elxy)",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.16.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.6"
  },
  "scripts": {
    "test": "tree-sitter test",
    "build": "tree-sitter generate --no-bindings && node-gyp build"
  },
  "tree-sitter": [
    {
      "scope": "source.md",
      "injection-regex": "markdown|md",
      "file-types": [
        "md"
      ]
    }
  ]
}
